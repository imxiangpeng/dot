{
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "vscode-neovim.neovimExecutablePaths.linux": "/usr/local/bin/nvim",
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "C_Cpp.intelliSenseEngine": "disabled",
    "git.openRepositoryInParentFolders": "never",
    "vscode-neovim.ctrlKeysForInsertMode": [
      "a",
      "c",
      "d",
      "h",
      "j",
      "o",
      "r",
      "t",
      "u",
      "w",
      "e",
      "f",
      "b",
      "n",
      "p",
      "x",
      "y"
    ],
    "editor.inlayHints.enabled": "off",
    "files.exclude": {
      "**/.cache": true,
      "out*/**": true
    },
    "files.watcherExclude": {
      "**/.cache/**": true,
      "**/out*/**": true
    },
    "workbench.colorCustomizations": {

    },
    "editor.minimap.enabled": false,
    "editor.cursorBlinking": "solid",
    "explorer.excludeGitIgnore": true,
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}${rootPath}${separator}${profileName}${separator}${appName}",
    "vim.easymotion": true,
    "vim.useSystemClipboard": true,
    "cmake.showOptionsMovedNotification": false,
    "files.trimFinalNewlines": true,
    "files.eol": "\n",
    "task.autoDetect": "off",
    "launch": {


      "configurations": [],
      "compounds": []
    },
    "clangd.arguments": [
      "-j=1"
    ],
    "update.mode": "manual",
    "editor.stickyTabStops": true,
    "extensions.ignoreRecommendations": true,
    "postman.telemetry.enabled": false
}